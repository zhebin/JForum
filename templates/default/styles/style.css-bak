	FONT-WEIGHT: bold; COLOR: #069
}
.helpbox {
	BACKGROUND: #ddd; MARGIN-BOTTOM: 10px; WIDTH: 100%
}
.helpbox LI {
	LIST-STYLE-TYPE: none
}
.helpbox TD {
